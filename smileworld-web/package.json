{
  "name": "smileworld",
  "version": "1.0.0",
  "author": "xiong_xin_xin@outlook.com ",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "test": "vue-cli-service build --mode test",
    "release": "vue-cli-service build --mode release",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@aspnet/signalr": "^1.1.4",
    "axios": "^0.18.1",
    "core-js": "^2.6.5",
    "countup.js": "^1.9.3",
    "view-design": "^4.0.0",
    "vue": "^2.6.6",
    "vue-countup-v2": "^1.0.3",
    "vue-easytable": "^2.21.9",
    "vue-router": "^3.0.2",
    "vue-table-with-tree-grid": "^0.2.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "mockjs": "^1.0.1-beta3",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "prettier/prettier": "off",
      "vue/no-parsing-error": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "postcss-cssnext": {},
      "postcss-import": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
